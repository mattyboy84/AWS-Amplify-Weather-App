{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "next",
        "next/core-web-vitals"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style": "off",
        "no-console": "off",
        "max-len": "off",
        "camelcase": "off",
        "no-await-in-loop": "off",
        "no-underscore-dangle": "off",
        "array-callback-return": "off",
        "no-promise-executor-return": "off"
    }
}
